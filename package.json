{
    "name": "@asnc/ts_hook",
    "version": "0.1.5",
    "description": "Run ts file in node",
    "scripts": {
        "build": "tsc -p ./tsconfig.build.json",
        "publishTest": "pnpm publish --no-git-checks --dry-run",
        "ci:build": "tsc -p ./tsconfig.build.json",
        "ci:test": "vitest run",
        "test:ui": "vitest --ui"
    },
    "devDependencies": {
        "@swc/core": "^1.3.52",
        "@types/node": "^20.1.1",
        "@vitest/ui": "^0.33.0",
        "glob": "^10.2.2",
        "typescript": "^4.9.4",
        "vitest": "^0.31.0"
    },
    "dependencies": {
        "source-map-support": "^0.5.21"
    },
    "engines": {
        "node": ">=16"
    },
    "author": "Eavid",
    "license": "MIT",
    "files": [
        "out/**/*.?([mc])js",
        "hook.mjs",
        "require.cjs"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/asnow-c/ts-hook"
    },
    "keywords": [
        "TypeScript",
        "node",
        "node loader"
    ]
}
