{
  "name": "@asnc/ts_hook",
  "version": "0.2.1",
  "description": "Run ts file in node",
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "publishTest": "pnpm publish --no-git-checks --dry-run",
    "ci:build": "tsc -p ./tsconfig.build.json",
    "ci:test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@swc/core": "^1.7.11",
    "@types/node": "^20.16.0",
    "@vitest/ui": "^2.0.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "Eavid",
  "license": "MIT",
  "files": [
    "out/**/*.?([mc])js",
    "hook.mjs",
    "require.cjs"
  ],
  "exports": {
    ".": "./hook.mjs",
    "./hook.mjs": "./hook.mjs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ieavid/ts-hook"
  },
  "keywords": [
    "TypeScript",
    "node",
    "node loader"
  ]
}
